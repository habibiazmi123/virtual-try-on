{
  "env": {
    "es6": true
  },
  "extends": ["next/core-web-vitals", "prettier", "eslint:recommended", "plugin:react/recommended", "plugin:react-hooks/recommended"],
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "eqeqeq": "warn",
    "react/prop-types": "off",
    "@next/next/no-img-element": "off",
    "prefer-const": "warn",
    "no-use-before-define": "error",
    "import/no-cycle": "error"
  }
}
